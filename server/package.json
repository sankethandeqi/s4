    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "express-async-errors": "^3.1.1",
    "joi": "^14.0.6",
    "jsonwebtoken": "^8.4.0",
    "kue": "^0.11.6",
    "multer": "^1.4.1",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "nodemon": "^1.18.5",
    "sequelize": "^4.41.0"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-plugin-node": "^8.0.0"
